{
    "name": "@lootsystem/character",
    "version": "1.0.0",
    "description": "Sistema de personajes para MMORPG",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch"
    },
    "dependencies": {
        "@lootsystem/core": "*",
        "@lootsystem/races": "*",
        "@lootsystem/classes": "*"
    },
    "devDependencies": {
        "tsup": "^8.0.1",
        "typescript": "^5.3.3"
    }
}